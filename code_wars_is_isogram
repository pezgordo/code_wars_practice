""""
def is_isogram(string):
    string = string.lower()
    for i in string:
        
        if string.count(i) > 1:

            return False

    return True
            
"""
        
def is_isogram(string):
    return len(string) == len(set(string.lower()))

        
    
        

   
print(is_isogram("Dermatoglyphics"))
print(is_isogram("isogram"))
print(is_isogram("aba"))
print(is_isogram("moOse"))
print(is_isogram("isIsogram"))
print(is_isogram(""))


